    <modelVersion>4.0.0</modelVersion>
    <groupId>com.chandler</groupId>
    <artifactId>chandler25-jdk17-demo</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <description>Demo project for Spring Boot</description>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>3.3.6</version>
        <relativePath/>
    </parent>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>17</java.version>

        <spring-cloud.version>2023.0.4</spring-cloud.version>
        <spring-cloud-alibaba.version>2023.0.3.2</spring-cloud-alibaba.version>

        <mybatis-plus.version>3.5.12</mybatis-plus.version>
        <boot-mybatis.version>3.0.5</boot-mybatis.version>
        <boot-druid.version>1.2.11</boot-druid.version>
        <boot-mapper.version>4.2.3</boot-mapper.version>
        <mysql.version>8.0.29</mysql.version>
        <springdoc.version>2.5.0</springdoc.version>

        <hutool.version>5.8.26</hutool.version>
        <guava.version>31.1-jre</guava.version>
        <fastjson.version>2.0.3</fastjson.version>
        <springfox.version>3.0.0</springfox.version>
        <lombok.version>1.18.24</lombok.version>
        <netty-version>4.1.77.Final</netty-version>
        <lang3.version>3.12.0</lang3.version>
    </properties>


